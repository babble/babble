<html>
  <body>

    <%
    var theFile = request.getFile( "theFile" );
if ( theFile ){
    db._files.save( theFile );
    print( theFile._id + "<BR>" );
    print( "<img src='/~f?id=" + theFile._id + "'>" );
}

    %>
    
    <form method="POST" enctype="multipart/form-data" >
      <input type="hidden" name="MAX_FILE_SIZE" value="<?= 1024 * 1024 * 100 ?>" />
      Choose a file to upload: <input name="theFile" type="file" /><br />
      <input type="submit" value="Upload File" />
    </form>
    
  </body>
</html>
