<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >

<suite name="corejs" >

  <test name="content">
	<parameter name="js-dir-name"  value="/data/corejs/content/test"/>
	<classes>
      <class name="ed.js.engine.JSTestFactory"/>
    </classes>
  </test>
    
    <test name="core">
      <parameter name="js-dir-name"  value="/data/corejs/core/test"/>
      <parameter name="js-ex-regex"  value=".*test_routes.js"/>
      <classes>
        <class name="ed.js.engine.JSTestFactory"/>
      </classes>
    </test>

    <test name="core-routes">
      <parameter name="js-dir-name"  value="/data/corejs/core/test"/>
      <parameter name="js-inc-regex"  value=".*test_routes.js"/>
      <parameter name="js-ex-regex"  value=".*"/>
      <parameter name="security-bypass"  value="true"/>
      <classes>
        <class name="ed.js.engine.JSTestFactory"/>
      </classes>
    </test>

    <test name="db">
      <parameter name="js-dir-name"  value="/data/corejs/db/test"/>
      <classes>
        <class name="ed.js.engine.JSTestFactory"/>
      </classes>
    </test>

    <test name="ext">
      <parameter name="js-dir-name"  value="/data/corejs/ext/test"/>
      <classes>
        <class name="ed.js.engine.JSTestFactory"/>
      </classes>
    </test>

    <test name="io">
      <parameter name="js-dir-name"  value="/data/corejs/io/test"/>
      <classes>
        <class name="ed.js.engine.JSTestFactory"/>
      </classes>
    </test>

    <test name="net">
      <parameter name="js-dir-name"  value="/data/corejs/net/test"/>
      <classes>
        <class name="ed.js.engine.JSTestFactory"/>
      </classes>
    </test>

    <test name="sandbox">
      <parameter name="js-dir-name"  value="/data/corejs/sandbox/test"/>
      <classes>
        <class name="ed.js.engine.JSTestFactory"/>
      </classes>
    </test>

    <test name="ui-chart">
      <parameter name="js-dir-name"  value="/data/corejs/ui/chart/test"/>
      <classes>
        <class name="ed.js.engine.JSTestFactory"/>
      </classes>
    </test>

    <test name="util">
      <parameter name="js-dir-name"  value="/data/corejs/util/test"/>
      <classes>
        <class name="ed.js.engine.JSTestFactory"/>
      </classes>
    </test>

</suite>

